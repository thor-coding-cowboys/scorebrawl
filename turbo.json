{
	"$schema": "https://v2-8-1.turborepo.dev/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**", "**/*.tsbuildinfo"],
			"env": [
				"BETTER_AUTH_SECRET",
				"GITHUB_CLIENT_SECRET",
				"VITE_GITHUB_CLIENT_ID",
				"GOOGLE_CLIENT_SECRET",
				"VITE_GOOGLE_CLIENT_ID"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"outputs": []
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"wrangler-typecheck": {
			"dependsOn": ["build"],
			"outputs": []
		}
	}
}
