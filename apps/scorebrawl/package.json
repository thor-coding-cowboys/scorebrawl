{
  "name": "scorebrawl",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev-e2e": "bun --env-file=.env.e2e run dev",
    "drizzle-studio": "bun --env-file=.env.local drizzle-kit studio --config ./drizzle.config.ts",
    "drizzle-studio-e2e": "bun --env-file=.env.e2e drizzle-kit studio --config ./drizzle.config.ts",
    "drizzle-migrate": "bun --env-file=.env.local drizzle-kit migrate",
    "drizzle-migrate-e2e": "bun --env-file=.env.e2e drizzle-kit migrate",
    "drizzle-generate": "bun --env-file=.env.local drizzle-kit generate",
    "db-start": "docker compose up -d --wait",
    "db-stop": "docker compose down",
    "build": "next build --debug",
    "start": "next start",
    "test": "bun test",
    "trigger": "bunx trigger.dev@latest dev",
    "trigger-deploy": "bunx trigger.dev@latest deploy"
  },
  "dependencies": {
